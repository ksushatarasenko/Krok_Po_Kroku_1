<div class="question-set" id="set3">
    <div class="dialog italic ">
        <div class="colors">
            <p class="p22 italic ">(ja) <input type="text" data-answer="rysuję" class="line_2"></p>
        </div>
        <div class="colors">
            <p class="p22">(ty) <input type="text" data-answer="rysujesz" class="line_2">
            </p>
        </div>
        <div class="colors">
            <p class="p22"> (on/ ona/ ono) <input type="text" data-answer="rysuje" class="line_2">
            </p>
        </div>
        <div class="colors">
            <p class="p22"> (my) <input type="text" data-answer="rysujemy" class="line_2">
            </p>
        </div>
        <div class="colors">
            <p class="p22"> (wy) <span class="blue">rysujecie</span>
            </p>
        </div>
        <div class="colors">
            <p class="p22"> (oni/ one) <input type="text" data-answer="rysują" class="line_2"></p>
        </div>
    </div>
    <button onclick="checkAnswers('set3')">Check</button>
    <button onclick="toggleHints('hints3')">Wskazówka</button>
    <div id="hints3" style="display: none;">
        <p>ja => rysuję
        </p>
        <p>ty => rysujesz</p>
        <p>on/ona/ono => rysuje</p>
        <p>my => rysujemy</p>
        <p>wy => rysujecie</p>
        <p>oni/one => rysują</p>
    </div>
</div>

